{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "short_name": "creeptd-stats",
    "version": "1.3.1",
    "homepage_url": "https://github.com/bochsdbg/creeptd-stats",
    "description": "__MSG_extensionDescription__",
    "icons": {
        "16": "icons/16.png",
        "24": "icons/24.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "96": "icons/96.png",
        "128": "icons/128.png",
        "256": "icons/256.png"
    },
    "browser_action": {
        "default_icon": "icons/32.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.creeptd.com/*/games/*"
            ],
            "js": [
                "inject.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "creeptd-stats.js",
        "dygraph.js",
        "style.css"
    ],
    "default_locale": "en"
}